.direct_supply
  width 100%
  background whitesmoke
  .title
    float left
    width 100%
    height 50px
    font-size 20px
    line-height 50px
    text-align center
    i
      font-size 22px
  .direct_supply_good
    float left
    width 100%
    height 238px
    padding 0 10px
    div
      width 172px
      height 112px
      float left
      position relative
      background #f4f4f4
      margin 4px 4px
      section
        box-sizing border-box
        padding-top 5px
        padding-left 5px
        width 100%
        display inline-block
        img
          margin-top 3px
          height 16px
          display block
      >img
        position absolute
        top 0
        left 0
        height 110px



